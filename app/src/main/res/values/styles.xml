<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Holo.NoActionBar.Fullscreen">
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="checkbtnStyle" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/selector_chkbox</item>
    </style>

    <style name="NoticeDialogStyle" parent="android:Theme.Holo.NoActionBar.Fullscreen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowFrame">@null</item>
    </style>
</resources>